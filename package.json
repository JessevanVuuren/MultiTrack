{
  "name": "multitrack",
  "version": "0.0.1",
  "description": "A plug and play audio plugin for Motion Canvas",
  "homepage": "https://github.com/JessevanVuuren/MultiTrack#readme",
  "bugs": {
    "url": "https://github.com/JessevanVuuren/MultiTrack/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JessevanVuuren/MultiTrack.git"
  },
  "license": "MIT",
  "author": "JessevanVuuren",
  "type": "module",
  "module": "./dist/vite-plugin.js",
  "types": "./dist/vite-plugin.d.ts",
  "files": [
    "./dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/vite-plugin.cjs",
      "import": {
        "default": "./dist/vite-plugin.js",
        "types": "./dist/vite-plugin.d.ts"
      }
    },
    "./editor-plugin": {
      "require": "./dist/editor-plugin.cjs",
      "import": {
        "default": "./dist/editor-plugin.js",
        "types": "./dist/editor-plugin.d.ts"
      }
    },
    "./exporter": {
      "require": "./dist/exporter.cjs",
      "import": {
        "default": "./dist/exporter.js",
        "types": "./dist/exporter.d.ts"
      }
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "typescript": "^5.8.2",
    "vite": "^5.4.19",
    "vite-plugin-dts": "^4.5.4"
  },
  "peerDependencies": {
    "@motion-canvas/2d": "^3.17.2",
    "@motion-canvas/core": "^3.17.2",
    "@motion-canvas/ui": "^3.17.2",
    "@motion-canvas/vite-plugin": "^3.17.2"
  }
}
