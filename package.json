{
  "name": "motion-canvas-multitrack",
  "version": "0.1.0",
  "description": "A plug and play audio plugin for Motion Canvas",
  "homepage": "https://github.com/JessevanVuuren/MultiTrack#readme",
  "bugs": {
    "url": "https://github.com/JessevanVuuren/MultiTrack/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JessevanVuuren/MultiTrack.git"
  },
  "license": "MIT",
  "author": "JessevanVuuren",
  "type": "module",
  "module": "./dist/vite-plugin.js",
  "types": "./dist/vite-plugin.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "motion-canvas",
    "multitrack",
    "audio-editing",
    "animation",
    "audios"
  ],
  "exports": {
    ".": {
      "require": "./dist/vite-plugin.cjs",
      "import": {
        "default": "./dist/vite-plugin.js",
        "types": "./dist/vite-plugin.d.ts"
      }
    },
    "./editor-plugin": {
      "import": {
        "default": "./dist/src/index.js",
        "types": "./dist/src/index.d.ts"
      }
    },
    "./exporter": {
      "import": {
        "default": "./dist/src/exporter/index.js",
        "types": "./dist/src/exporter/index.d.ts"
      }
    }
  },
  "typesVersions": {
    "*": {
      "editor-plugin": [
        "./dist/src/index.d.ts"
      ],
      "exporter": [
        "./dist/src/exporter/index.d.ts"
      ]
    }
  },
  "scripts": {
    "dev": "vite",
    "prepare": "npm run build",
    "build": "tsc && vite build && tsc -p tsconfig.client.json",
    "watch": "concurrently \"tsc --watch\" \"tsc -p tsconfig.client.json --watch\" \"npm run watch:yalc\"",
    "watch:yalc": "nodemon --watch dist --delay 1000ms --exec \"yalc push\"",
    "yalc:push": "npm run build && yalc push"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.11",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.8.2",
    "vite": "^5.4.19",
    "vite-plugin-dts": "^4.5.4"
  },
  "peerDependencies": {
    "@motion-canvas/2d": "^3.17.2",
    "@motion-canvas/core": "^3.17.2",
    "@motion-canvas/ui": "^3.17.2",
    "@motion-canvas/vite-plugin": "^3.17.2"
  }
}